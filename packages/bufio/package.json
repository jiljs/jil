{
  "name": "@jil/bufio",
  "description": "Buffer and serialization utilities for javascript",
  "version": "0.1.3",
  "keywords": [
    "buffer",
    "io",
    "serialization",
    "read",
    "write"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "build": "nb-tsc",
    "clean": "nb-clean jil-bufio*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest",
    "verify": "npm pack && tar xf jil-bufio*.tgz && tree package && npm run clean"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.1",
    "@nutol/eslint-config": "^0.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "benchmark": "^2.1.4",
    "random-string": "^0.2.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./dist/custom": "./dist/custom-browser.js"
  }
}
