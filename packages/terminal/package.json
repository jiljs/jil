{
  "name": "@jil/terminal",
  "description": "A collection of utilities for managing and interacting with a terminal",
  "version": "0.1.1",
  "keywords": [
    "jil",
    "terminal",
    "term",
    "utils",
    "ink"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "build": "nb-tsc",
