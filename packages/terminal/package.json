    "figures": "^4.0.0",
    "slice-ansi": "^5.0.0",
    "string-width": "^5.1.0",
    "strip-ansi": "^7.0.1",
    "supports-hyperlinks": "^2.2.0",
    "term-size": "^3.0.1",
    "wrap-ansi": "^8.0.1"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.2",
    "@nutol/eslint-config": "^0.2.1",
    "@types/jest": "^27.4.0",
