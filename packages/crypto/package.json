{
  "name": "@jil/crypto",
  "description": "TS crypto library based on bcrypto",
  "version": "0.1.1",
  "keywords": [
    "crypto",
    "crypt",
    "hash",
    "sign",
    "verify",
    "encrypt",
    "decrypt",
    "encode",
    "decode"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "version": "npm run copy:assets",
    "copy:assets": "cpy package.json README.md dist",
    "build": "nb-tsc && npm run copy:assets",
    "clean": "nb-clean jil-crypto*.tgz dist/* *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest --runInBand",
    "verify": "npm pack && tar xf jil-crypto*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@jil/bufio": "^0.1.1",
    "bcrypto": "^5.4.0"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.1",
    "@nutol/eslint-config": "^0.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "cpy-cli": "^3.1.1",
    "jest-environment-node-single-context": "^27.2.1"
  },
  "files": [
    "**/*",
    "!__tests__"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "browser": {
    "./aead": "./aead-browser.js",
    "./aes": "./aes-browser.js",
    "./arc4": "./arc4-browser.js",
    "./bcrypt": "./bcrypt-browser.js",
    "./blake2b": "./blake2b-browser.js",
    "./blake2s": "./blake2s-browser.js",
    "./bn": "./bn-browser.js",
    "./chacha20": "./chacha20-browser.js",
    "./cipher": "./cipher-browser.js",
    "./cleanse": "./cleanse-browser.js",
    "./ctr-drbg": "./ctr-drbg-browser.js",
    "./dsa": "./dsa-browser.js",
    "./eb2k": "./eb2k-browser.js",
    "./ed25519": "./ed25519-browser.js",
    "./ed448": "./ed448-browser.js",
    "./encoding/base16": "./encoding/base16-browser.js",
    "./encoding/base32": "./encoding/base32-browser.js",
    "./encoding/base58": "./encoding/base58-browser.js",
    "./encoding/base64": "./encoding/base64-browser.js",
    "./encoding/bech32": "./encoding/bech32-browser.js",
    "./encoding/cash32": "./encoding/cash32-browser.js",
    "./gost94": "./gost94-browser.js",
    "./hash160": "./hash160-browser.js",
    "./hash256": "./hash256-browser.js",
    "./hash-drbg": "./hash-drbg-browser.js",
    "./hkdf": "./hkdf-browser.js",
    "./hmac-drbg": "./hmac-drbg-browser.js",
    "./internal/custom": "./internal/custom-browser.js",
    "./internal/pgpdf": "./internal/pgpdf-browser.js",
    "./keccak": "./keccak-browser.js",
    "./md2": "./md2-browser.js",
    "./md4": "./md4-browser.js",
    "./md5": "./md5-browser.js",
    "./md5sha1": "./md5sha1-browser.js",
    "./murmur3": "./murmur3-browser.js",
    "./p192": "./p192-browser.js",
    "./p224": "./p224-browser.js",
    "./p256": "./p256-browser.js",
    "./p384": "./p384-browser.js",
    "./p521": "./p521-browser.js",
    "./pbkdf2": "./pbkdf2-browser.js",
    "./poly1305": "./poly1305-browser.js",
    "./random": "./random-browser.js",
    "./ripemd160": "./ripemd160-browser.js",
    "./rsa": "./rsa-browser.js",
    "./salsa20": "./salsa20-browser.js",
    "./schnorr": "./schnorr-browser.js",
    "./scrypt": "./scrypt-browser.js",
    "./secp256k1": "./secp256k1-browser.js",
    "./secretbox": "./secretbox-browser.js",
    "./sha1": "./sha1-browser.js",
    "./sha224": "./sha224-browser.js",
    "./sha256": "./sha256-browser.js",
    "./sha384": "./sha384-browser.js",
    "./sha512": "./sha512-browser.js",
    "./sha3": "./sha3-browser.js",
    "./siphash": "./siphash-browser.js",
    "./whirlpool": "./whirlpool-browser.js",
    "./x25519": "./x25519-browser.js",
    "./x448": "./x448-browser.js"
  }
}
