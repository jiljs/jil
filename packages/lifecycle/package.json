{
  "name": "@jil/lifecycle",
  "description": "A light lifecycle management library",
  "version": "0.1.0",
  "keywords": [
    "lifecycle",
    "disposable",
    "dispose",
    "abstract",
    "base",
    "mutable",
    "ref",
    "count",
    "reference",
    "tracker",
    "trackable"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "copy:assets": "cpy package.json README.md dist",
    "build": "nb-tsc && npm run copy:assets",
    "clean": "nb-clean jil-lifecycle*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest",
    "verify": "npm pack && tar xf jil-lifecycle*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@jil/types": "^0.1.0",
    "tily": "^2.10.1"
  },
  "devDependencies": {
    "@jil/types": "^0.1.0",
    "@nutol/build": "^0.2.0",
    "@nutol/eslint-config": "^0.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "cpy-cli": "^3.1.1"
  },
  "files": [
    "**/*",
    "!__tests__"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
