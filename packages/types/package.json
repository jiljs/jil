{
  "name": "@jil/types",
  "description": "A basic TypeScript types library",
  "version": "0.2.0",
  "keywords": [
    "ts",
    "typescript",
    "types",
    "constructor",
    "mixin",
    "target",
    "mixin-target"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "acceptance": "nb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "nb-tsc",
    "clean": "nb-clean jil-types*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest",
    "verify": "npm pack && tar xf jil-types*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "ts-essentials": "^9.1.2"
  },
  "peerDependencies": {
    "typescript": ">=4.1.0"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.1",
    "@nutol/eslint-config": "^0.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
