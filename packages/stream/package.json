{
  "name": "@jil/stream",
  "description": "A light stream from vscode",
  "version": "0.1.0",
  "keywords": [
    "stream",
    "readable",
    "writable"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "build": "nb-tsc",
    "clean": "nb-clean jil-stream*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest",
    "verify": "npm pack && tar xf jil-stream*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@jil/errors": "^0.2.1",
    "@jil/lifecycle": "^0.2.3"
  },
  "devDependencies": {
    "@jil/async": "^0.2.3",
    "@nutol/build": "^0.2.0",
    "@nutol/eslint-config": "^0.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
