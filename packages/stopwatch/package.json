{
  "name": "@jil/stopwatch",
  "description": "A managed stopwatch for both nodejs and browser",
  "version": "0.2.0",
  "keywords": [
    "stop",
    "watch",
    "stopwatch",
    "performance",
    "time",
    "calculate"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
