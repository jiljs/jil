{
  "name": "@jil/pipeline",
  "description": "Pipe an input through a series of routines and tasks to produce an output, or simply, run logic in a series of stages",
  "version": "0.1.5",
  "keywords": [
    "jil",
    "pipeline",
    "routine",
    "task",
    "work",
    "work unit",
    "parallel",
    "serial"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "build": "nb-tsc",
    "clean": "nb-clean 'jil-pipeline*.tgz' 'dist/*' '*.tsbuildinfo' 'package'",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest --runInBand",
    "verify": "npm pack && tar xf jil-pipeline*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@jil/common": "^0.2.26",
    "@jil/debug": "^0.2.6",
    "@jil/support": "^0.2.5",
    "@jil/translate": "^0.1.6",
    "@types/split": "^1.0.0",
    "execa": "^5.1.1",
    "lodash": "^4.17.21",
    "split": "^1.0.1",
    "tily": "^2.14.4"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.6",
    "@nutol/eslint-config": "^0.2.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
